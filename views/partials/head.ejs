		}

		.alert-padding {
			display: none;
		}
		
		.home-main-content {
		    background: url(img/home_background.png) no-repeat center center; 
		    background: url(img/home_background.png) no-repeat center center, -webkit-linear-gradient(top left, #312a6c, #852d91);
		    background: url(img/home_background.png) no-repeat center center, -moz-linear-gradient(top left, #312a6c, #852d91);
			background-size: cover;
			margin: 0;
			padding: 0;
			height: 300px;
			color: #ececec;
			display: flex;
		}

		.home-main-content > .container {
			display: flex;
			flex-direction: column;
			align-self: center;
		}
</style>
<script>
	function toastNotification(text, type) {
		if (type == "danger") {
			$.notify("<i class=\"fas fa-exclamation-triangle\"></i> " + text, {
				delay: 300,
				animate: {
					enter: 'animated fadeInUp',
					exit: 'animated fadeOutDown'
				},
				type: type
			});
		} else if (type == "success") {
		 	$.notify("<i class=\"fas fa-check\"></i> " + text, {
		 		delay: 300,
				animate: {
					enter: 'animated fadeInUp',
					exit: 'animated fadeOutDown'
				},
				type: type
			});
		}
	}
</script>